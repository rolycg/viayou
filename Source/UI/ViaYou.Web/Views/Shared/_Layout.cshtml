<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>ViaYou</title>

    @Styles.Render("~/Content/Shared/css")
    @Styles.Render("~/Content/BasicTemplate/css")

    <link rel="shortcut icon" href="~/Content/BasicTemplate/images/ico/favicon.ico">
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="~/Content/BasicTemplate/images/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="~/Content/BasicTemplate/images/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="~/Content/BasicTemplate/images/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="~/Content/BasicTemplate/images/ico/apple-touch-icon-57-precomposed.png">

    @Scripts.Render("~/bundles/modernizr")

</head>
<body>

    @Html.Partial("_HeaderAndMenu")
    <div class="container">
        @RenderBody()
    </div>
    @*@Html.Partial("_Footer")*@

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/Scripts/BasicTemplate/js")
    @RenderSection("scripts", required: false)
</body>
</html>
